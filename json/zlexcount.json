{
  "ZLEXCOUNT": {
    "summary": "Count the number of members in a sorted set between a given lexicographical range",
    "complexity": "O(log(N)) with N being the number of elements in the sorted set.",
    "group": "sorted_set",
    "since": "2.8.9",
    "arity": 4,
    "command_flags": [
      "readonly",
      "fast"
    ],
    "acl_categories": [
      "read",
      "sortedset",
      "fast"
    ],
    "key_specs": [
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "range": {
            "lastkey": 0,
            "step": 1,
            "limit": 0
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "the number of elements in the specified score range.",
        "type": "integer"
      }
    ],
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "min",
        "type": "string"
      },
      {
        "name": "max",
        "type": "string"
      }
    ]
  }
}