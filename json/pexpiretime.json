{
  "PEXPIRETIME": {
    "summary": "Get the expiration Unix timestamp for a key in milliseconds",
    "complexity": "O(1)",
    "group": "generic",
    "since": "7.0.0",
    "arity": 2,
    "command_flags": [
      "readonly",
      "random",
      "fast"
    ],
    "acl_categories": [
      "keyspace",
      "read",
      "fast"
    ],
    "key_specs": [
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "range": {
            "lastkey": 0,
            "step": 1,
            "limit": 0
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "Expiration Unix timestamp in milliseconds, or a negative value in order to signal an error (see the description below).",
        "type": "integer"
      }
    ],
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ]
  }
}