{
  "SETSLOT": {
    "summary": "Bind a hash slot to a specific node",
    "complexity": "O(1)",
    "group": "cluster",
    "since": "3.0.0",
    "arity": -4,
    "container": "CLUSTER",
    "command_flags": [
      "admin",
      "random",
      "stale"
    ],
    "acl_categories": [
      "admin",
      "slow",
      "dangerous"
    ],
    "return_types": [
      {
        "description": "All the subcommands return `OK` if the command was successful. Otherwise an error is returned.",
        "type": "simple-string"
      }
    ],
    "arguments": [
      {
        "name": "slot",
        "type": "integer"
      },
      {
        "name": "subcommand",
        "type": "enum",
        "enum": [
          "IMPORTING",
          "MIGRATING",
          "STABLE",
          "NODE"
        ]
      },
      {
        "name": "node-id",
        "type": "string",
        "optional": true
      }
    ]
  }
}