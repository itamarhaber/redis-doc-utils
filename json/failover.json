{
  "FAILOVER": {
    "summary": "Start a coordinated failover between this server and one of its replicas.",
    "group": "server",
    "since": "6.2.0",
    "arity": -1,
    "command_flags": [
      "admin",
      "noscript",
      "stale"
    ],
    "acl_categories": [
      "admin",
      "slow",
      "dangerous"
    ],
    "return_types": [
      {
        "description": "`OK` if the command was accepted and a coordinated failover is in progress. An error if the operation cannot be executed.",
        "type": "simple-string"
      }
    ],
    "arguments": [
      {
        "name": "target",
        "type": "block",
        "optional": true,
        "block": [
          {
            "command": "TO"
          },
          {
            "name": "host",
            "type": "string"
          },
          {
            "name": "port",
            "type": "integer"
          },
          {
            "command": "FORCE",
            "optional": true
          }
        ]
      },
      {
        "command": "ABORT",
        "optional": true
      },
      {
        "command": "TIMEOUT",
        "name": "milliseconds",
        "type": "integer",
        "optional": true
      }
    ]
  }
}