{
    "PAUSE": {
        "summary": "Stop processing commands from clients for some time",
        "complexity": "O(1)",
        "group": "connection",
        "since": "2.9.50",
        "arity": -3,
        "container": "CLIENT",
        "function": "clientCommand",
        "history": [
            [
                "3.2.10",
                "Client pause prevents client pause and key eviction as well."
            ],
            [
                "6.2",
                "CLIENT PAUSE WRITE mode added along with the `mode` option."
            ]
        ],
        "command_flags": [
            "admin",
            "noscript",
            "loading",
            "stale"
        ],
        "acl_categories": [
            "connection"
        ],
        "arguments": [
            {
                "name": "timeout",
                "type": "integer"
            },
            {
                "name": "mode",
                "type": "oneof",
                "optional": true,
                "arguments": [
                    {
                        "type": "pure-token",
                        "token": "WRITE"
                    },
                    {
                        "type": "pure-token",
                        "token": "ALL"
                    }
                ]
            }
        ]
    }
}
