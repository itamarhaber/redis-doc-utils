                                "token": "MAXLEN"
                            },
                            {
                                "name": "minid",
                                "type": "pure-token",
                                "token": "MINID"
                            }
                        ]
                    },
                    {
                        "name": "operator",
                        "type": "oneof",
                        "value": [
                            {
                                "name": "equal",
                                "type": "pure-token",
                                "token": "="
                            },
                            {
                                "name": "approximately",
                                "type": "pure-token",
                                "token": "~"
                            }
                        ],
                        "optional": true
                    },
                    {
                        "name": "threshold",
                        "type": "string",
                        "value": "threshold"
                    },
                    {
                        "token": "LIMIT",
