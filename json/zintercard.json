{
  "ZINTERCARD": {
    "summary": "Intersect multiple sorted sets and return the cardinality of the result",
    "complexity": "O(N*K) worst case with N being the smallest input sorted set, K being the number of input sorted sets.",
    "group": "sorted_set",
    "since": "7.0.0",
    "arity": -3,
    "command_flags": [
      "readonly",
      "movablekeys"
    ],
    "acl_categories": [
      "read",
      "sortedset",
      "slow"
    ],
    "key_specs": [
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "keynum": {
            "keynumidx": 0,
            "firstkey": 1,
            "step": 1
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "the number of elements in the resulting intersection.",
        "type": "integer"
      }
    ],
    "arguments": [
      {
        "name": "numkeys",
        "type": "integer"
      },
      {
        "name": "key",
        "type": "key",
        "multiple": true
      }
    ]
  }
}