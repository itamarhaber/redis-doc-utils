{
    "PSUBSCRIBE": {
        "summary": "Listen for messages published to channels matching the given patterns",
        "complexity": "O(N) where N is the number of patterns the client is already subscribed to.",
        "group": "pubsub",
        "since": "2.0.0",
        "arity": -2,
        "function": "psubscribeCommand",
        "command_flags": [
            "pubsub",
            "noscript",
            "loading",
            "stale",
            "sentinel"
        ],
        "acl_categories": [
            "pubsub",
            "slow"
        ],
        "arguments": [
            {
                "name": "pattern",
                "type": "block",
                "value": [
                    {
                        "name": "pattern",
                        "type": "pattern",
                        "value": "pattern"
                    }
                ],
                "multiple": true
            }
        ]
    }
}