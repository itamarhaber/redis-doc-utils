{
  "HGET": {
    "summary": "Get the value of a hash field",
    "complexity": "O(1)",
    "group": "hash",
    "since": "2.0.0",
    "arity": 3,
    "command_flags": [
      "readonly",
      "fast"
    ],
    "acl_categories": [
      "read",
      "hash",
      "fast"
    ],
    "key_specs": [
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "range": {
            "lastkey": 0,
            "step": 1,
            "limit": 0
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "the value associated with `field`, or `nil` when `field` is not",
        "type": "bulk-string"
      }
    ],
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "field",
        "type": "string"
      }
    ]
  }
}