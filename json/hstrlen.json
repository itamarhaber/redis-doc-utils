{
  "HSTRLEN": {
    "summary": "Get the length of the value of a hash field",
    "complexity": "O(1)",
    "group": "hash",
    "since": "3.2.0",
    "arity": 3,
    "function": "hstrlenCommand",
    "command_flags": [
      "readonly",
      "fast"
    ],
    "acl_categories": [
      "read",
      "hash",
      "fast"
    ],
    "key_specs": [
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "range": {
            "lastkey": 0,
            "step": 1,
            "limit": 0
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "the string length of the value associated with `field`, or zero when `field` is not present in the hash or `key` does not exist at all.",
        "type": "integer"
      }
    ],
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "field",
        "type": "string"
      }
    ]
  }
}