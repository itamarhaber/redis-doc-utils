    "group": "sorted_set",
    "since": "2.0.0",
    "arity": -4,
    "command_flags": [
      "write",
      "denyoom",
      "movablekeys"
    ],
    "acl_categories": [
      "write",
      "sortedset",
      "slow"
    ],
    "key_specs": [
      {
        "flags": [
          "write"
        ],
        "begin_search": {
          "index": {
            "pos": 1
          }
        },
        "find_keys": {
          "range": {
            "lastkey": 0,
            "step": 1,
            "limit": 0
          }
        }
      },
      {
        "flags": [
          "read"
        ],
        "begin_search": {
          "index": {
            "pos": 2
          }
        },
        "find_keys": {
          "keynum": {
            "keynumidx": 0,
            "firstkey": 1,
            "step": 1
          }
        }
      }
    ],
    "return_types": [
      {
        "description": "the number of elements in the resulting sorted set at",
